image: rancher/configmap-reload:{{build.tag}}
manifests:
  -
    image: rancher/configmap-reload:{{build.tag}}-linux-amd64
    platform:
      architecture: amd64
      os: linux
  -
    image: rancher/configmap-reload:{{build.tag}}-windows-1809
    platform:
      architecture: amd64
      os: windows
      version: 1809
  -
    image: rancher/configmap-reload:{{build.tag}}-windows-1903
    platform:
      architecture: amd64
      os: windows
      version: 1903
